@(ndoc,"1.0")
{
    styles:{
        //import("*.tcss"),
        "" :{
            color:blue,
            font-size:10,
        }
        title:{
            font-size:50,
            bold:true
            color:red,
        }
    }

    @define course{
        page(name:'course'){
            circle(),
        }
    }

    course{
        @(title) text("My Title",at:top),
        rectangle(
            size:(50,50),
            background:$palette[3],
            rotate:45,
            color:$palette[1],
//            stroke:basic(dash:[30],width:5)
            stroke:sloppy
        )
        equation("""
            x=\frac{-b \pm \sqrt {b^2-4ac}}{2a}
            """,
            at:(50,50),
            origin:center){

        },
    },
    page{
        @(title) text("My Title Page 2",at:top),
        equation("""
            x=\frac{+b \pm \sqrt {b^2-4ac}}{2a}
            """,
            at:(50,50),
            origin:center){

        },
    },
}
