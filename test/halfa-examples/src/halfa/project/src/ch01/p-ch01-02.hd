@(halfa,"1.0")
{
    import("sty/*.sty.tson"),
    import("lib/*.comp.tson"),
    page(extends:'cover'){
        text['title']("My Title"),
        equation("""
            x=\frac{-b \pm \sqrt {b^2-4ac}}{2a}
            """){
            at:(50,50),
            anchor:'center'
        },
    },
    page(extends:'normal'){
        text['title']("My Title"),
        equation("""
            x=\frac{-b \pm \sqrt {b^2-4ac}}{2a}
            """){
            at:(50,50),
            anchor:'center'
        },
    },
    page(extends:'conclusion'){
        text['title']("My Title"),
        equation("""
            x=\frac{-b \pm \sqrt {b^2-4ac}}{2a}
            """){
            at:(50,50),
            anchor:'center'
        },
    },
    import("*.tson"),
}
