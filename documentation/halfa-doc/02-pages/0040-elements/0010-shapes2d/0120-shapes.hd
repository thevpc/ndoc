@normal page{
    @header {
        text("Shapes"),
    }
    @body{
        grid((6,3),columns-weight:[1,3]){
            styles:{
                "*": {margin:2}
                source:{
                    at:left
                }
            }
            // Row
            stack(background:white,draw-contour){
                rectangle(at:center, size:(50,20), background:$palette[4], color:$palette[2])
            }
            source(tson, """ rectangle() """)

            // Row
            stack(background:white,draw-contour){
                triangle(at:center, size:50, background:$palette[4], color:$palette[2])
            }
            source(tson, """ triangle() """)

            // Row
            stack(background:white,draw-contour){
                square(at:center, size:50, background:$palette[4], color:$palette[2])
            }
            source(tson, """ square() """)

            // Row
            stack(background:white,draw-contour){
                rhombus(at:center, size:50, background:$palette[4], color:$palette[2])
            }
            source(tson, """
                rhombus()
                diamond()
                """)

            // Row
            stack(background:white,draw-contour){
                parallelogram(at:center, size:50, background:$palette[4], color:$palette[2])
            }
            source(tson, """
                parallelogram()
                """)

            // Row
            stack(background:white,draw-contour){
                trapezoid(at:center, size:50, background:$palette[4], color:$palette[2])
            }
            source(tson, """
                trapezoid()
                """)

            // Row
            stack(background:white,draw-contour){
                circle(at:center, size:50, background:$palette[4], color:$palette[2])
            }
            source(tson, """
                circle()
                """)

            // Row
            stack(background:white,draw-contour){
                ellipse(at:center, size:(80,30), background:$palette[4], color:$palette[2])
            }
            source(tson, """
                ellipse(size:(80,30))
                """)

        }
    }

}