normal(title:"Plot lines"){
body{
        grid((2,1),columns-weight:[1,3]){
            styles:{
                "*": {margin:2}
                source:{
                    at:left
                }
            }
            // Row
            group(background:white,draw-contour){
                plot2d(xmin:-5 , xmax: 5){
                        curve{
                            f(x):sin(x)
                        }
                }
            }
            source(tson,
                """
                    plot2d(xmin:-5 , xmax: 5){
                        curve {
                            f(x):sin(x)
                        }
                    }
                """
            )
            // Row
            group(background:white,draw-contour){
                plot2d(xmin:-5 , xmax: 5){
                    curve{
                        f(x):sin(x)
                        title:'sin'
                        color:red
                    }
                    curve{
                        f(x):cos(x)
                        title:'cos'
                        color:blue
                    }
                }
            }
            source(tson,
                """
                plot2d(xmin:-5 , xmax: 5){
                    curve{
                        f(x):sin(x)
                        title:'sin'
                        color:red
                    }
                    curve{
                        f(x):cos(x)
                        title:'cos'
                        color:blue
                    }
                }
                """
            )
        }
    }

}
